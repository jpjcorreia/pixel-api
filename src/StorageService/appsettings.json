{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "RabbitMq": "amqp://guest:guest@rabbitmq:5672"
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "blockWhenFull": true,
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "/tmp/visits.log",
                "rollingInterval": "Infinite",
                "outputTemplate": "{Message}{NewLine}"
              }
            }
          ]
        }
      }
    ]
  },
  "AllowedHosts": "*"
}

