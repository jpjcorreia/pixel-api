{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "RabbitMq": "amqp://guest:guest@rabbitmq:5672"
  },
  "Storage": {
    "File": {
      "Path": "/tmp/visits.log"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "/tmp/visits.log",
          "rollingInterval": "Infinite",
          "outputTemplate": "{Message}{NewLine}"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}

